<extension xmlns="http://ns.adobe.com/air/extension/19.0">
  <id>com.marpies.ane.googleplayservices.games</id>
  <versionNumber>1.0.0</versionNumber>
  <platforms>

    <platform name="Android-ARM">
        <applicationDeployment>
            <nativeLibrary>libGooglePlayServicesGames.jar</nativeLibrary>
            <initializer>com.marpies.ane.googleplayservices.games.GooglePlayServicesGamesExtension</initializer>
            <finalizer>com.marpies.ane.googleplayservices.games.GooglePlayServicesGamesExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-x86">
        <applicationDeployment>
            <nativeLibrary>libGooglePlayServicesGames.jar</nativeLibrary>
            <initializer>com.marpies.ane.googleplayservices.games.GooglePlayServicesGamesExtension</initializer>
            <finalizer>com.marpies.ane.googleplayservices.games.GooglePlayServicesGamesExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="default">
        <applicationDeployment/>
    </platform>
  </platforms>
</extension>
