<extension xmlns="http://ns.adobe.com/air/extension/20.0">
  <id>com.marpies.ane.googleplayservices.ads</id>
  <versionNumber>1</versionNumber>
  <platforms>
    <platform name="Android-ARM">
        <applicationDeployment>
            <nativeLibrary>libGooglePlayServicesAds.jar</nativeLibrary>
            <initializer>com.marpies.ane.googleplayservices.ads.GooglePlayServicesAdsExtension</initializer>
            <finalizer>com.marpies.ane.googleplayservices.ads.GooglePlayServicesAdsExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-x86">
        <applicationDeployment>
            <nativeLibrary>libGooglePlayServicesAds.jar</nativeLibrary>
            <initializer>com.marpies.ane.googleplayservices.ads.GooglePlayServicesAdsExtension</initializer>
            <finalizer>com.marpies.ane.googleplayservices.ads.GooglePlayServicesAdsExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="default">
        <applicationDeployment/>
    </platform>
  </platforms>
</extension>
