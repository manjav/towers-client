<extension xmlns="http://ns.adobe.com/air/extension/20.0">
  <id>com.marpies.ane.androidsupport</id>
  <versionNumber>1</versionNumber>
  <platforms>

    <platform name="Android-ARM">
        <applicationDeployment>
            <nativeLibrary>libAndroidSupport.jar</nativeLibrary>
            <initializer>com.marpies.ane.androidsupport.AndroidSupportExtension</initializer>
            <finalizer>com.marpies.ane.androidsupport.AndroidSupportExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-x86">
        <applicationDeployment>
            <nativeLibrary>libAndroidSupport.jar</nativeLibrary>
            <initializer>com.marpies.ane.androidsupport.AndroidSupportExtension</initializer>
            <finalizer>com.marpies.ane.androidsupport.AndroidSupportExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="default">
        <applicationDeployment/>
    </platform>
  </platforms>
</extension>
